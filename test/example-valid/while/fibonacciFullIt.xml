<unit>
  <main>
    <str>"This program calculates the nth fibonacci number iteratively."</str>
    <print><kind>str</kind><newline>true</newline></print>
    <str>"Please enter n (should not be too large): "</str>
    <print><kind>str</kind><newline>false</newline></print>
    <addr_local>0</addr_local>
    <num>0</num>
    <assign_do/>
    <addr_local>0</addr_local>
    <read><ty>int</ty></read>
    <str>"The input n is "</str>
    <print><kind>str</kind><newline>false</newline></print>
    <local>0</local>
    <print><kind>i32</kind><newline>true</newline></print>
    <str>"The nth fibonacci number is "</str>
    <print><kind>str</kind><newline>false</newline></print>
    <addr_local>1</addr_local>
    <num>0</num>
    <assign_do/>
    <addr_local>2</addr_local>
    <num>1</num>
    <assign_do/>
    <addr_local>3</addr_local>
    <num>0</num>
    <assign_do/>
    <while>
      <cond><local>0</local><num>0</num><op>gt</op></cond>
      <body>
        <addr_local>3</addr_local>
        <local>1</local>
        <assign_do/>
        <addr_local>1</addr_local>
        <local>2</local>
        <assign_do/>
        <addr_local>2</addr_local>
        <local>3</local>
        <local>2</local>
        <op>add</op>
        <assign_do/>
        <addr_local>0</addr_local>
        <local>0</local>
        <num>1</num>
        <op>sub</op>
        <assign_do/>
      </body>
    </while>
    <local>1</local>
    <print><kind>i32</kind><newline>true</newline></print>
  </main>
</unit>