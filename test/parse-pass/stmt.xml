<unit>
  <fn>
    <name>do_if</name>
    <args/>
    <body>
      <if>
        <cond><num>1</num></cond>
        <when><num>1</num><return/></when>
        <else><num>0</num><return/></else>
      </if>
    </body>
  </fn>
  <fn>
    <name>do_while</name>
    <args/>
    <body>
      <while>
        <cond><num>1</num></cond>
        <body>
          <if><cond><num>0</num></cond><when><num>1</num><return/></when><else/></if>
        </body>
      </while>
    </body>
  </fn>
  <fn>
    <name>do_signles</name>
    <args/>
    <body>
      <num>1</num>
      <print><kind>i32</kind><newline>false</newline></print>
      <num>1</num>
      <print><kind>i32</kind><newline>true</newline></print>
      <num>1</num>
      <exit/>
      <num>1</num>
      <return/>
      <block><num>2</num><return/></block>
    </body>
  </fn>
  <main/>
</unit>